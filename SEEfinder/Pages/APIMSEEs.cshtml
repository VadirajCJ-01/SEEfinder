<div class="card text-center">
    <table class="table  table-striped table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SEEsList[0].FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SEEsList[0].LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SEEsList[0].Vertical)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SEEsList[0].ContactDetails)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SEEsList[0].EmailID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.SEEsList)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Vertical)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ContactDetails)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EmailID)
                </td>
            </tr>
        }
    </tbody>
</table>
