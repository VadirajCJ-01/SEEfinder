}

<div class="card text-left">
<form action="" method="post">
    <label asp-for="Vertical">
    </label>
        <select asp-for="Vertical" asp-items="Model.Verticals" name="SelectedVertical" id="SelectedVertical" class="form-select">
    </select>
        <button type="submit" class="btn btn-primary">select</button>
</form>
